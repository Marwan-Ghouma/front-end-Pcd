    "gs-api:download": "cp /Users/medrh/IdeaProjects/backend/backend/target/backend-0.0.1-SNAPSHOT.jar ./tools/swagger/dist && cp /Users/medrh/IdeaProjects/backend/backend/target/swagger.json ./tools/swagger/src",
    "gs-api:extract": "node ./tools/swagger/src/swagger.json 0.0.1-SNAPSHOT",
    "gs-api:swagger-gen": "./node_modules/.bin/ng-swagger-gen -i ./tools/swagger/src/swagger.json -o src/gs-api/src"
